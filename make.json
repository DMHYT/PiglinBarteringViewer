        "abis": [
            "armeabi-v7a",
            "x86"
        ],
        "linkNative": [
            "minecraftpe",
            "innercore",
            "nativejs"
        ],
        "excludeFromRelease": [],
        "pushTo": "storage/emulated/0/games/horizon/packs/Inner_Core_1/innercore/mods/PiglinBarteringViewer",
